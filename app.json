{
  "expo": {
    "name": "MrHydrateMe",
    "slug": "MrHydrateMe",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/droplet.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "permissions": {
        "NOTIFICATIONS": "Allow notifications for hydration reminders."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/droplet.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.asadullahjan.MrHydrateMe"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/droplet.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/droplet.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "3b596c1f-bb7a-4265-9078-ba3e84858191"
      }
    }
  }
}
